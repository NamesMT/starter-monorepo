{
  "name": "starter-monorepo",
  "type": "module",
  "version": "4.4.0",
  "private": true,
  "packageManager": "pnpm@10.26.1",
  "engines": {
    "node": ">=20.13.1"
  },
  "scripts": {
    "lint": "turbo run lint",
    "test:types": "turbo run test:types",
    "quickcheck": "turbo run quickcheck",
    "dev": "dotenvx run -- turbo run dev:prepare && dotenvx run -- turbo run dev",
    "dev:noConvex": "dotenvx run -- turbo run dev --filter=!backend-convex",
    "devSST": "dotenvx run -f .env.sst -f .env -- sst dev \"pnpm -F=frontend run dev\"",
    "build": "dotenvx run -- turbo run build",
    "deploy": "dotenvx run -- turbo run deploy",
    "i18n": "pnpm dlx lingo.dev run",
    "repo-release": "pnpm dlx repo-release --release --bump --push --pkg=",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "pathe": "catalog:",
    "sst": "catalog:",
    "std-env": "catalog:"
  },
  "devDependencies": {
    "@antfu/eslint-config": "catalog:",
    "@dotenvx/dotenvx": "catalog:",
    "@namesmt/utils": "catalog:",
    "@types/aws-lambda": "catalog:",
    "@types/node": "catalog:",
    "@unocss/eslint-plugin": "catalog:",
    "eslint": "catalog:",
    "frontend": "workspace:*",
    "lint-staged": "catalog:",
    "simple-git-hooks": "catalog:",
    "tsx": "catalog:",
    "turbo": "catalog:",
    "typescript": "catalog:"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
