{
  "name": "backend",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "imports": {
    "#src/*": "./src/*"
  },
  "scripts": {
    "lint": "eslint .",
    "test": "vitest",
    "test:types": "tsc --noEmit --skipLibCheck",
    "quickcheck": "pnpm run lint && pnpm run test:types",
    "check": "pnpm lint && pnpm test:types && vitest run --coverage",
    "dev": "dotenvx run -f .env.local.ignored -f .env.local -f .env -- tsx watch src/dev.ts",
    "stub": "unbuild --stub",
    "build": "dotenvx run -f .env.prod.ignored -f .env.prod -f .env -- unbuild",
    "deploy": "dotenvx run -f .env.prod.ignored -f .env.prod -f .env -- sh scripts/deploy.sh"
  },
  "dependencies": {
    "srvx": "^0.8.0"
  },
  "devDependencies": {
    "@hono/arktype-validator": "^2.0.1",
    "@kinde-oss/kinde-typescript-sdk": "^2.11.0",
    "@local/common": "workspace:*",
    "@local/locales": "workspace:*",
    "@local/tsconfig": "workspace:*",
    "@namesmt/utils": "^0.5.14",
    "@rollup/plugin-dynamic-import-vars": "^2.1.5",
    "@scalar/hono-api-reference": "^0.9.4",
    "@vitest/coverage-v8": "^3.2.3",
    "arktype": "^2.1.20",
    "backend-convex": "workspace:*",
    "consola": "^3.4.2",
    "convex": "^1.24.8",
    "hono": "^4.7.11",
    "hono-adapter-aws-lambda": "^1.3.3",
    "hono-openapi": "^0.4.8",
    "hono-sessions": "^0.7.2",
    "petite-vue-i18n": "^11.1.5",
    "std-env": "^3.9.0",
    "unbuild": "^3.5.0",
    "vitest": "^3.2.3"
  }
}
